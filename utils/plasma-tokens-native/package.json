{
    "name": "@salutejs/plasma-tokens-native",
    "version": "1.0.0",
    "description": "Plasma tokens for swift, kotlin, react-native",
    "private": true,
    "files": ["build"],
    "repository": {
        "type": "git",
        "url": "git+https://github.com/salute-developers/plasma.git"
    },
    "keywords": ["plasma", "tokens", "ios", "react-native", "kotlin"],
    "scripts": {
        "prepare": "npm run build",
        "prebuild": "mkdir -p data && cp -R node_modules/@salutejs/plasma-tokens/data/* data",
        "build": "npx ts-node ./src/generate.ts",
        "postbuild": "npx ts-node ./config.ts"
    },
    "devDependencies": {
        "@salutejs/plasma-tokens": "1.49.0",
        "@salutejs/plasma-tokens-utils": "0.23.0",
        "@salutejs/plasma-typo": "0.33.0",
        "style-dictionary": "^3.7.2",
        "styled-components": "^5.3.10",
        "ts-node": "^10.9.1"
    },
    "author": "Salute Frontend Team <salute.developers@gmail.com>",
    "publishConfig": {
        "access": "public"
    },
    "bugs": {
        "url": "https://github.com/salute-developers/plasma/issues"
    }
}
