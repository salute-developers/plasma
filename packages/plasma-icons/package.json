{
    "name": "@salutejs/plasma-icons",
    "version": "1.137.0",
    "description": "Salute Design System Icons",
    "main": "index.js",
    "module": "es/index.js",
    "types": "index.d.ts",
    "repository": {
        "type": "git",
        "url": "ssh://git@github.com:salute-developers/plasma.git"
    },
    "author": "Salute Frontend Team <salute.developers@gmail.com>",
    "peerDependencies": {
        "@salutejs/plasma-core": "^1.0.0",
        "react": ">=16.13.1",
        "react-dom": ">=16.13.1",
        "styled-components": "^5.1.1"
    },
    "devDependencies": {
        "@babel/cli": "^7.15.4",
        "@babel/core": "^7.15.5",
        "@babel/plugin-transform-react-constant-elements": "^7.17.12",
        "@babel/preset-env": "^7.15.4",
        "@babel/preset-react": "^7.14.5",
        "@babel/preset-typescript": "^7.15.0",
        "@salutejs/plasma-core": "1.108.0",
        "@types/node": "^16.7.13",
        "@types/react": "^18.0.28",
        "@types/react-dom": "^18.0.11",
        "@types/styled-components": "^5.1.14",
        "babel-loader": "^8.2.2",
        "babel-plugin-annotate-pure-calls": "0.4.0",
        "babel-plugin-styled-components": "^1.13.2",
        "babel-plugin-transform-class-properties": "6.24.1",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "styled-components": "^5.3.1",
        "typescript": "^4.0.8"
    },
    "publishConfig": {
        "access": "public"
    },
    "scripts": {
        "prepare": "npm run build",
        "prebuild": "rm -rf ./es ./Icon.assets ./Icons index.js ./Icon* ./index.* ./scalable",
        "build": "npm run build:cjs && npm run build:esm",
        "postbuild": "tsc --outDir . --emitDeclarationOnly",
        "build:cjs": "BABEL_ENV=cjs SC_NAMESPACE=plasma babel ./src --out-dir . --extensions .ts,.tsx",
        "build:esm": "BABEL_ENV=esm SC_NAMESPACE=plasma babel ./src --out-dir ./es --extensions .ts,.tsx"
    },
    "files": [
        "Icon.assets",
        "Icons",
        "index.d.ts",
        "index.js",
        "Icon.d.ts",
        "Icon.js",
        "IconRoot.d.ts",
        "IconRoot.js",
        "es"
    ],
    "contributors": [
        "Vasiliy Loginevskiy",
        "Антонов Игорь Александрович",
        "Виноградов Антон Александрович",
        "Зубаиров Фаниль Асхатович",
        "Чельцов Евгений Олегович"
    ],
    "sideEffects": false
}
