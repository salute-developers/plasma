        "build:esm": "tsc -p ./tsconfig.es.json",
        "lint": "../../node_modules/.bin/eslint ./src/additional.ts ./src/general.ts ./src/index.ts --ext .ts --quiet"
    },
    "files": ["dist", "es"],
    "contributors": ["Vasiliy Loginevskiy", "Anton Vinogradov", "Fanil Zubairov"],
    "devDependencies": {
        "typescript": "^4.0.8"
    }
