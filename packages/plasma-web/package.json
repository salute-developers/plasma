{
  "name": "@salutejs/plasma-web",
  "version": "1.507.0-dev.0",
  "description": "Salute Design System / React UI kit for web applications",
  "author": "Salute Frontend Team <salute.developers@gmail.com>",
  "license": "MIT",
  "keywords": [
    "design-system",
    "react-components",
    "ui-kit",
    "react"
  ],
  "main": "index.js",
  "module": "es/index.js",
  "types": "index.d.ts",
  "repository": {
    "type": "git",
    "url": "ssh://git@github.com:salute-developers/plasma.git",
    "directory": "packages/plasma-web"
  },
  "dependencies": {
    "@salutejs/plasma-core": "1.191.0",
    "@salutejs/plasma-hope": "1.329.0",
    "@salutejs/plasma-new-hope": "0.251.0-dev.0",
    "@salutejs/plasma-themes": "0.28.0",
    "@salutejs/plasma-tokens-b2b": "1.45.0",
    "@salutejs/plasma-tokens-b2c": "0.56.0",
    "@salutejs/plasma-tokens-web": "1.61.0",
    "@salutejs/plasma-typo": "0.41.0"
  },
  "peerDependencies": {
    "@salutejs/plasma-icons": "^1.0.0",
    "react": ">=16.13.1",
    "react-dom": ">=16.13.1",
    "styled-components": "^5.1.1"
  },
  "devDependencies": {
    "@babel/cli": "7.24.1",
    "@babel/core": "7.24.4",
    "@babel/plugin-transform-class-properties": "7.24.1",
    "@babel/plugin-transform-react-constant-elements": "7.24.1",
    "@babel/preset-env": "7.24.4",
    "@babel/preset-react": "7.24.1",
    "@babel/preset-typescript": "7.24.1",
    "@ironkinoko/rollup-plugin-styles": "4.0.3",
    "@linaria/rollup": "5.0.4",
    "@microsoft/api-extractor": "7.38.3",
    "@rollup/plugin-babel": "6.0.4",
    "@rollup/plugin-commonjs": "25.0.7",
    "@rollup/plugin-node-resolve": "15.2.3",
    "@salutejs/plasma-colors": "0.14.0",
    "@salutejs/plasma-cy-utils": "0.122.0",
    "@salutejs/plasma-icons": "1.210.0",
    "@salutejs/plasma-sb-utils": "0.191.0",
    "@storybook/addon-docs": "7.6.17",
    "@storybook/addon-essentials": "7.6.17",
    "@storybook/addons": "7.6.17",
    "@storybook/react": "7.6.17",
    "@storybook/react-vite": "7.6.17",
    "@storybook/theming": "7.6.17",
    "@types/node": "16.7.13",
    "@types/react": "18.0.28",
    "@types/react-dom": "18.0.11",
    "@types/styled-components": "5.1.0",
    "@vitejs/plugin-react": "3.1.0",
    "babel-loader": "8.2.2",
    "babel-plugin-annotate-pure-calls": "0.4.0",
    "babel-plugin-styled-components": "1.13.2",
    "csstype": "3.1.3",
    "default-browser-id": "2.0.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "rollup": "3.28.0",
    "storybook": "7.6.17",
    "styled-components": "5.3.1",
    "typescript-coverage-report": "0.7.0",
    "vite": "4.5.3"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "preapi-report": "npm run generate:typings",
    "api-report": "npx api-extractor run --local",
    "prepare": "npm run build",
    "prebuild": "rm -rf ./components ./es ./helpers ./hocs ./hooks ./mixins ./tokens ./types ./utils index.d.ts index.d.ts.map index.js index.js.map",
    "build": "npm run build:cjs && npm run build:esm && npm run build:css",
    "postbuild": "npm run generate:typings",
    "build:cjs": "BABEL_ENV=cjs SC_NAMESPACE=plasma-web babel ./src --out-dir . --extensions .ts,.tsx",
    "build:esm": "BABEL_ENV=esm SC_NAMESPACE=plasma-web babel ./src --out-dir ./es --extensions .ts,.tsx",
    "prebuild:css": "rm -rf src-css && ./scripts/copy-linaria-components.sh",
    "build:css": "BABEL_ENV=esm SC_NAMESPACE=plasma-web rollup -c",
    "postbuild:css": "rm -rf src-css",
    "generate:typings": "tsc --outDir . --emitDeclarationOnly",
    "storybook": "storybook dev -p ${PORT:-7007} -c .storybook",
    "storybook:build": "storybook build -c .storybook -o build-sb",
    "typescript-coverage": "npx typescript-coverage-report > /dev/null",
    "lint": "../../node_modules/.bin/eslint ./src --ext .js,.ts,.tsx --quiet"
  },
  "typeCoverage": {
    "ignoreFiles": [
      "src/**/*.component-test.tsx",
      "src/**/*.stories.tsx",
      "src/**/*.perftest.tsx"
    ],
    "atLeast": 99.96
  },
  "contributors": [
    "Vasiliy Loginevskiy",
    "Anton Vinogradov",
    "Fanil Zubairov"
  ],
  "sideEffects": false
}