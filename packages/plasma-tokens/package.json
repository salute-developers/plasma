{
    "name": "@salutejs/plasma-tokens",
    "version": "1.53.0",
    "description": "",
    "author": "Salute Frontend Team <salute.developers@gmail.com>",
    "module": "es/index.js",
    "main": "index.js",
    "types": "index.d.ts",
    "scripts": {
        "prepare": "npm run build",
        "prebuild": "rm -rf ./build ./colors ./es ./properties ./src ./brands ./themes ./themesValues ./typo ./typography ./typographyValues ./index.* ./theme.json",
        "build": "npm run generate && npm run build:cjs && npm run build:esm && npm run lint",
        "build:cjs": "tsc",
        "build:esm": "tsc -p ./tsconfig.es.json",
        "generate": "npx ts-node ./lib/generate.ts",
        "figma": "npm run extract --prefix='./design-language'",
        "lint": "../../node_modules/.bin/eslint --ext .js,.ts,.tsx ./src --fix --quiet"
    },
    "publishConfig": {
        "access": "public"
    },
    "files": [
        "colors",
        "brands",
        "es",
        "themes",
        "themesValues",
        "typo",
        "typography",
        "typographyValues",
        "index.d.ts",
        "index.d.ts.map",
        "index.js",
        "index.js.map",
        "theme.json"
    ],
    "peerDependencies": {
        "styled-components": "^5.1.1"
    },
    "devDependencies": {
        "@diez/web-sdk-common": "10.6.0",
        "@salutejs/plasma-colors": "0.7.0",
        "@salutejs/plasma-tokens-utils": "0.25.0",
        "@salutejs/plasma-typo": "0.33.0",
        "@types/node": "16.7.13",
        "jsdom": "17.0.0",
        "jsdom-global": "3.0.2",
        "styled-components": "5.3.6",
        "ts-node": "10.2.1",
        "typescript": "4.0.8"
    },
    "contributors": [
        "Виноградов Антон Александрович",
        "Зубаиров Фаниль Асхатович",
        "Vasiliy Loginevskiy"
    ],
    "sideEffects": false
}
