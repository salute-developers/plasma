    "@salutejs/plasma-icons": "^1.0.0",
    "@salutejs/plasma-tokens": "^1.0.0",
    "@salutejs/plasma-ui": "^1.0.0",
    "react": ">=16.13.1",
    "react-dom": ">=16.13.1",
    "styled-components": "^5.1.1"
  },
  "devDependencies": {
    "@babel/cli": "7.15.4",
